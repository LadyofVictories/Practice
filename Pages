def gen_fib():
  count=int(input("How many Fibonacci numbers would you like to generate?"))
  i=1
  if count==0:
    fib=[]

  elif count==1:
    fib=[1]
  elif count==2:
    fib=[1,1]
  elif count>2:
    fib=[1,1]
    while i<(count-1):
      fib.append(fib[i]+fib[i-1])
      i +=1
      
  return fib



def dedup(x):
  y=[]
  for i in x:
    if i not in y:
      y.append(i)
  return y




def dedup(x):
  return list(set(x))

